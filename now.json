{
    "version": 2,
    "builds": [
        { "src": "serverless.js", "use": "@now/node" }
    ],
    "routes": [
        { "src": "/.*", "dest": "/serverless.js" }
    ],
    "env": { 
        "VERSION": "@version",
        "LOG_LEVEL": "@log_level",
        "PODCASTS_API_KEY": "@podcasts_api_key"
    }
}